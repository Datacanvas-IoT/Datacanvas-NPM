# Example .env file for DataCanvas SDK
# Copy this file to .env and fill in your actual credentials

# Your DataCanvas API credentials
DATACANVAS_CLIENT_KEY=your_access_key_client_here
DATACANVAS_SECRET_KEY=your_access_key_secret_here
DATACANVAS_PROJECT_ID=7

# Optional: API URL (defaults to http://localhost:3001)
DATACANVAS_API_URL=http://localhost:3001
